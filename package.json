{
  "name": "labs-common-tools",
  "version": "1.0.38",
  "description": "These are all common utilities",
  "main": "./dist/index",
  "typings": "dist/index",
  "types": "./dist/index.d.ts",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "preparepublish": "npm install && npm run build",
    "watch": "tsc --watch",
    "pretest": "rm -rf ./dist && ./node_modules/.bin/tsc ./test/**.ts --target es2017 --module commonjs --outDir ./dist --sourceMap --strict",
    "test": "_mocha ./dist/test/*.spec.js -- -R spec",
    "cover": "istanbul cover _mocha ./dist/test/*.spec.js -- -R spec",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:eslint": "eslint --fix './{src,test,tools}/**/*.ts'",
    "lint:prettier": "prettier --write './{src,lib,test,tools}/**/*.ts'",
    "check": "npm run check:prettier && npm run check:eslint",
    "check:eslint": "eslint './{src,test,lib,tools}/**/*.ts'",
    "check:prettier": "prettier -c './{src,test,lib,tools}/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/medesko/labs-common-tools"
  },
  "keywords": [],
  "author": "Mohamed KEITA",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.790.0",
    "aws-signed-axios": "^1.0.6",
    "aws-xray-sdk": "^3.2.0",
    "axios": "^0.21.0",
    "joi": "^17.3.0",
    "lambda-logger-middleware": "^1.0.3",
    "virgen-acl": "^0.0.22",
    "lodash": "^4.17.20",
    "middy": "^0.36.0",
    "middy-autoproxyresponse": "^0.1.0",
    "mongodb": "^3.6.3",
    "mongoose": "5.10.13",
    "ms": "^2.1.2",
    "nunjucks": "^3.2.2",
    "nunjucks-date": "^1.5.0",
    "nunjucks-date-filter": "^0.1.1",
    "pino": "^6.7.0",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.64",
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.2.14",
    "@types/chai-as-promised": "^7.1.3",
    "@types/lodash": "^4.14.165",
    "@types/mocha": "^8.0.4",
    "@types/mongodb": "^3.5.33",
    "@types/mongoose": "^5.10.0",
    "@types/node": "^14.14.7",
    "@types/nunjucks": "^3.1.3",
    "@types/uuid": "^8.3.0",
    "@types/hapi__joi": "^17.1.6",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "@types/pino": "^6.3.3",
    "aws-sdk-mock": "^5.1.0",
    "awscred": "^1.5.0",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.13.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-standard": "^16.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-standard": "^4.1.0",
    "flamebearer": "^1.1.3",
    "husky": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-config-prettier": "^6.15.0",
    "istanbul": "^1.1.0-alpha.1",
    "lint-staged": "^10.5.1",
    "madge": "^4.0.1",
    "mocha": "^8.2.1",
    "prettier": "^2.1.2",
    "proxyquire": "^2.1.3",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test && npm audit && npm run build"
    }
  }
}
