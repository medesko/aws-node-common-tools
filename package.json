{
		"name": "sls-common-tools",
		"version": "1.7.1",
		"description": "These are all common utilities",
		"main": "./dist/index.js",
		"types": "./dist/index.d.ts",
		"type": "commonjs",
		"scripts": {
				"build": "tsc",
				"preparepublish": "npm install && npm run build",
				"watch": "tsc --watch",
				"test": "jest",
				"cover": "istanbul cover _mocha ./dist/test/*.spec.js -- -R spec",
				"lint": "npm run lint:prettier && npm run lint:eslint",
				"lint:eslint": "eslint --fix './{src,test,tools}/**/*.ts'",
				"lint:prettier": "prettier --write './{src,test,tools}/**/*.ts'",
				"check": "npm run check:prettier && npm run check:eslint",
				"check:eslint": "eslint './{src,test,tools}/**/*.ts'",
				"check:prettier": "prettier -c './{src,test,tools}/**/*.ts'"
		},
		"repository": {
				"type": "git",
				"url": "git+https://github.com/medesko/sls-common-tools"
		},
		"keywords": [],
		"author": "Mohamed KEITA",
		"license": "ISC",
		"dependencies": {
				"aws-sdk": "^2.694.0",
				"aws-signed-axios": "^1.0.6",
				"aws-xray-sdk": "^3.0.1",
				"axios": "^0.19.2",
				"lambda-logger-middleware": "^1.0.3",
				"lodash": "^4.17.15",
				"middy": "^0.36.0",
				"middy-autoproxyresponse": "^0.1.0",
				"mongodb": "^3.5.8",
				"mongoose": "^5.9.18",
				"ms": "^2.1.2",
				"nunjucks": "^3.2.1",
				"nunjucks-date": "^1.5.0",
				"nunjucks-date-filter": "^0.1.1",
				"pino": "^6.3.2",
				"uuid": "^8.1.0"
		},
		"devDependencies": {
				"@types/aws-lambda": "^8.10.55",
				"@types/axios": "^0.14.0",
				"@types/chai": "^4.2.11",
				"@types/chai-as-promised": "^7.1.2",
				"@typescript-eslint/eslint-plugin": "^3.2.0",
				"@typescript-eslint/parser": "^3.2.0",
				"@types/jest": "^26.0.0",
				"@types/lodash": "^4.14.155",
				"@types/mongodb": "^3.5.20",
				"@types/mongoose": "^5.7.23",
				"@types/node": "^14.0.13",
				"@types/nunjucks": "^3.1.3",
				"@types/uuid": "^8.0.0",
				"aws-sdk-mock": "^5.1.0",
				"awscred": "^1.5.0",
				"chai": "^4.2.0",
				"coveralls": "^3.1.0",
				"eslint-config-standard": "^14.1.1",
				"eslint-plugin-node": "^11.1.0",
				"eslint-plugin-promise": "^4.2.1",
				"eslint-plugin-standard": "^4.0.1",
				"istanbul": "^1.0.0-alpha.2",
				"proxyquire": "^2.1.3",
				"ts-loader": "^7.0.5",
				"ts-node": "^8.10.2",
				"typescript": "^3.9.5",
				"prettier": "^2.0.5",
				"eslint": "^7.2.0",
				"madge": "^3.9.1",
				"eslint-config-airbnb": "^18.1.0",
				"eslint-config-airbnb-base": "^14.1.0",
				"eslint-config-prettier": "^6.11.0",
				"eslint-plugin-import": "^2.21.2",
				"eslint-plugin-jsx-a11y": "^6.2.3",
				"eslint-plugin-prettier": "^3.1.3",
				"eslint-plugin-react": "^7.20.0",
				"eslint-plugin-react-hooks": "^4.0.4",
				"flamebearer": "^1.1.3",
				"husky": "^4.2.5",
				"jest": "^26.0.1",
				"lint-staged": "^10.2.9",
				"ts-jest": "^26.1.0"
		},
		"jest": {
				"preset": "ts-jest",
				"testEnvironment": "node",
				"testMatch": [
						"test/**/*.spec.ts"
				],
				"moduleFileExtensions": [
						"ts",
						"js"
				]
		},
		"husky": {
				"hooks": {
						"pre-commit": "lint-staged && npm audit"
				}
		},
		"lint-staged": {
				"./{src,test,tools}/**/*.ts": "npm run lint"
		}
}
